target 'UZBroadcast' do
  platform :ios, '10.0'
  use_frameworks!

  pod 'LFLiveKit', :git => "https://github.com/kennic/LFLiveKit.git"

end

target 'UZBroadcastExample' do
	platform :ios, '10.0'
	use_frameworks!
	
	pod 'LFLiveKit', :git => "https://github.com/kennic/LFLiveKit.git"
	pod 'SwiftIcons'
	
end
